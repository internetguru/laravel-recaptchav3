{
    "name": "internetguru/laravel-recaptchav3",
    "description": "Interent Guru Laravel reCAPTCHA v3 package",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "InternetGuru\\LaravelRecaptchaV3\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "tests/src/App/"
        }
    },
    "require": {
        "php": "^8.4",
        "livewire/livewire": "^3.0",
        "laravel/framework": "^9.0 || ^10.0 || ^11.0",
        "internetguru/laravel-common": "^2"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^9.5",
        "phpunit/php-code-coverage": "^11.0",
        "livewire/livewire": "^3.5",
        "internetguru/laravel-common": "^2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "InternetGuru\\LaravelRecaptchaV3\\RecaptchaV3ServiceProvider"
            ]
        }
    }
}
